{
  "manifest_version": 2,
  "name": "Dont record my Google searches",
  "version": "1.0",
  "description": "Deletes Google searches from your browser history",
  "permissions": ["alarms", "history"],
  "background": {
    "scripts": ["dont-record-my-search.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/128.png"
  },
  "icons": {
    "128": "icons/128.png"
  }
}
